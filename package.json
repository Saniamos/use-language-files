{
    "name": "use-lang",
    "displayName": "use-lang",
    "description": "Language Support for USE (Uni Bremen)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "use",
            "aliases": ["use", "use"],
            "extensions": [".use"],
            "configuration": "./syntaxes/use.language-configuration.json"
        },{
            "id": "soil",
            "aliases": ["soil", "soil"],
            "extensions": [".cmd"],
            "configuration": "./syntaxes/soil.language-configuration.json"
        }],
        "grammars": [{
            "language": "use",
            "scopeName": "source.javascript",
            "path": "./syntaxes/use.tmLanguage.json"
        },{
            "language": "soil",
            "scopeName": "source.javascript",
            "path": "./syntaxes/soil.tmLanguage.json"
        }]
    }
}